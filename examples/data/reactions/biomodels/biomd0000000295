MFtrn:  ->(-ki**n*(amp*tanh(-2*dawn + 2*time - 48*floor(time/24))*tanh(-2*dusk + 2*time - 48*floor(time/24)) - amp*tanh(-2*dawn + 2*time - 48*floor(time/24)) + amp*tanh(-2*dusk + 2*time - 48*floor(time/24)) - amp - 4*vs)/(4*(ki**n + (FN + FNp)**n))) MF
MFdeg: MF ->(vm/(MF + km)) 
FCtrl:  ->(MF*ks) FC
FCdeg: FC ->(vd) 
FCtrs: FC ->(k1n) FN
FCtrs_rev: FN ->(k2n) FC
FCptrl:  ->(MF*ksp) FCp
FCpdeg: FCp ->(vdp) 
FCptrs: FCp ->(k1np) FNp
FCptrs_rev: FNp ->(k2np) FCp
