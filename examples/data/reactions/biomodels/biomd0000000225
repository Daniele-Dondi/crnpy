vgk: GLC ->(Vgk*_comp*(GLC/Sgk)**hGK/(GLC*((GLC/Sgk)**hGK + 1))) G6P_F6P
vpfk: G6P_F6P ->(Vpfk*_comp*(F6P/Spfk)**(-FBP*(-hact + hpfk)/(Sfba*(FBP/Sfba + 1)) + hpfk)/(G6P_F6P*((F6P/Spfk)**(-FBP*(-hact + hpfk)/(Sfba*(FBP/Sfba + 1)) + hpfk) + ((FBP/Xpfk)**hx + 1)/(alpha**(-FBP*(-hact + hpfk)/(Sfba*(FBP/Sfba + 1)) + hpfk)*(FBP/Xpfk)**hx + 1)))) FBP
vfba: FBP ->(Vfba*_comp/(Sfba*(FBP/Sfba + 1))) G3P
