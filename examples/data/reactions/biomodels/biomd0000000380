T6P_phosphatase: trehalose_6_phosphate + water ->(cell*tpp_Vmax*tpp_shock**heat/(tpp_Kt6p*water*(1 + trehalose_6_phosphate/tpp_Kt6p))) phosphate + trehalose
T6P_synthase: UDP_glucose + glucose_6_phosphate ->(cell*tps_Vmax*tps_activity*tps_shock**heat/(tps_Kg6p*tps_Kudg*(UDP_glucose/tps_Kudg + 1)*(glucose_6_phosphate/tps_Kg6p + 1))) H_ + UDP + trehalose_6_phosphate
trehalase: trehalose + water ->(cell*nth_Vmax*nth_shock**heat/(nth_Ktrh*water*(1 + trehalose/nth_Ktrh))) 2glucose
UDP_glucose_phosphorylase: H_ + UTP + glucose_1_phosphate ->(cell*ugp_Vmax*ugp_shock**heat/(H_*ugp_Kg1p*ugp_Kutp*(UDP_glucose*glucose_1_phosphate/(ugp_Kg1p*ugp_Kiudg) + UDP_glucose*ugp_Kiutp/(ugp_Kiudg*ugp_Kutp) + UTP*glucose_1_phosphate/(ugp_Kg1p*ugp_Kutp) + UTP/ugp_Kutp + glucose_1_phosphate/ugp_Kg1p + ugp_Kiutp/ugp_Kutp))) UDP_glucose + diphosphate
