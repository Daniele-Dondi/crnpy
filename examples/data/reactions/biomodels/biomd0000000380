glucose_transport: glucose ->(0) glucose
glucose_transport_rev: glucose ->(0) glucose
hexokinase: ATP + glucose ->(cell*hxk_Vmax*hxk_shock**heat*(-ADP*glucose_6_phosphate/hxk_Keq + ATP*glucose)/(ATP*glucose*hxk_Katp*hxk_Kglc*(ADP/hxk_Kadp + ATP/hxk_Katp + 1)*(glucose/hxk_Kglc + glucose_6_phosphate/hxk_Kg6p + 1 + trehalose_6_phosphate/hxk_Kit6p))) ADP + H_ + glucose_6_phosphate
phosphoglucomutase: glucose_6_phosphate ->(cell*pgm_Kg1p*pgm_Vmax*pgm_shock**heat/(glucose_1_phosphate*pgm_Kg6p + glucose_6_phosphate*pgm_Kg1p + pgm_Kg1p*pgm_Kg6p)) glucose_1_phosphate
