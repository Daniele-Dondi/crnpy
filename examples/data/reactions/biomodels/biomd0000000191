Arginase: ARGin ->(Arginase_Vmaxarg*cytosol/(ARGin + Arginase_Kmarg*(1.0 + ORN/Arginase_Kioarg))) ORN
Arginine_transport: ARGex ->(extracellular*(ARGex*Arginine_transport_Vmaxhat/((ARGex + Arginine_transport_Kmhat)*(ARGin/Arginine_transport_Kmhat + 1.0 + ORN/Arginine_transport_Kiornhat)) + ARGex*Arginine_transport_Vmaxlat/((ARGex + Arginine_transport_Kmlat)*(ARGin/Arginine_transport_Kmlat + 1.0 + ORN/Arginine_transport_Kiornhat)))/ARGex) ARGin
Ornithine_efflux: ORN ->(cytosol*(ORN*Ornithine_efflux_Vmaxefflhat/((ORN + Ornithine_efflux_Kiornhat*(ARGin/Ornithine_efflux_Kmhat + 1.0))*(ARGex/Ornithine_efflux_Kmhat + 1.0)) + ORN*Ornithine_efflux_Vmaxeffllat/((ORN + Ornithine_efflux_Kmeffllat*(ARGin/Ornithine_efflux_Kmlat + 1.0))*(ARGex/Ornithine_efflux_Kmlat + 1.0)))/ORN) 
NOS: ARGin ->(NOS_Vmaxnos1*cytosol/(ARGin + NOS_Kmnos1)) 
ODC: ORN ->(ODC_Vmaxodc*cytosol/(ODC_Kmodc + ORN)) 
