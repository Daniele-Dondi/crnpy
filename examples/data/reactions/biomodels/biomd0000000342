Smad2_nuclear_export: Smad2n ->(Vnuc*kexp_Smad2) Smad2c
Smad4_nuclear_import: Smad4c ->(Vcyt*kimp_Smad4) Smad4n
Smad4_nuclear_export: Smad4n ->(Vnuc*kexp_Smad4) Smad4c
Smad2_phosphorylation: Smad2c ->(LRC_endo*Vcyt*kpho_Smad2) PSmad2c
PSmad2_nuclear_import: PSmad2c ->(Vcyt*kimp_Smad2) PSmad2n
PSmad2_nuclear_export: PSmad2n ->(Vnuc*kexp_Smad2) PSmad2c
Smad2_Smad4_complex_formation: PSmad2c + Smad4c ->(Vcyt*kon_Smads) PSmad2_Smad4_c
Smad2_Smad4_complex_formation_rev: PSmad2_Smad4_c ->(Vcyt*koff_Smads) PSmad2c + Smad4c
Smad2_Smad4_nuclear_import: PSmad2_Smad4_c ->(Vcyt*kimp_Smads) PSmad2_Smad4_n
Smad2_Smad4_dissociation: PSmad2_Smad4_n ->(Vnuc*koff_Smads) PSmad2n + Smad4n
Smad2_Smad4_dissociation_rev: PSmad2n + Smad4n ->(Vnuc*kon_Smads) PSmad2_Smad4_n
Smad2_dephosphorylation: PSmad2n ->(Vnuc*kdepho_Smad2) Smad2n
