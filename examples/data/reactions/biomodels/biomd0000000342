T1R_production: AA ->(Vcyt*k_T1R/AA) T1R_surf
T1R_internalization_to_early_endosome: T1R_surf ->(Vcyt*ki) T1R_endo
T1R_recycling_from_early_endosome: T1R_endo ->(Vcyt*kr) T1R_surf
T1R_constitutive_degradation: T1R_endo ->(Vcyt*kdeg_T1R) empty_degraded
T2R_production: AA ->(Vcyt*k_T2R/AA) T2R_surf
T2R_internalization_to_early_endosome: T2R_surf ->(Vcyt*ki) T2R_endo
T2R_recycling_from_early_endosome: T2R_endo ->(Vcyt*kr) T2R_surf
T2R_constitutive_degradation: T2R_endo ->(Vcyt*kdeg_T2R) empty_degraded
LRC_activation: T1R_surf + T2R_surf + TGF_beta_ex ->(Vcyt*ka_LRC) LRC_surf
LRC_internalization_to_early_endosome: LRC_surf ->(Vcyt*ki) LRC_endo
LRC_constitutive_degradation: LRC_endo ->(Vcyt*kdeg_LRC) empty_degraded
dissociation_of_LRC_in_endosome: LRC_endo ->(Vcyt*kdiss_LRC) T1R_endo + T2R_endo + TGF_beta_endo
TGF_beta_constitutive_degradation: TGF_beta_endo ->(Vcyt*kdeg_TGF_beta) empty_degraded
