r1:  ->(-CS*kdr1*(UV*n1 + 1) + ks1*(UV*n3*(FHY3 + HY5) + 1)) CS
r2:  ->(-RUP*kdr2 + ks2*(UCS*UV + 1)) RUP
r3: 2UVR8M ->(k1) UVR8D
r4: 2CS + 2UVR8M ->(ka1) UCS
r4_rev: UCS ->(kd1) 2CS + 2UVR8M
r5: CD + 2CS ->(ka2) CDCS
r5_rev: CDCS ->(kd2) CD + 2CS
r6: RUP + UVR8M ->(ka3) UR
r7: 2UR ->(kd3) 2RUP + UVR8D
r8: UVR8D ->(k2) 2UVR8M
r9:  ->(-HY5*kdr3*(CDCS/(CDCS + kdr3a) + CDW/(CDW + kdr3b) - UCS/(UCS + ksr)) + ks3*(UV*n2 + 1)) HY5
r10: CD + DWD ->(ka4) CDW
r10_rev: CDW ->(kd4) CD + DWD
