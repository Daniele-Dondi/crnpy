PDH: p3g ->(PDH_KAphp*PDH_KiAser*PDH_kcatA*cell*serA/((PDH_KiAser + ser)*(PDH_KAp3g*PDH_KAphp + PDH_KAp3g*php + PDH_KAphp*p3g))) php
PDH_rev: php ->(k_PDH_rev) p3g
PSA: php ->(PSA_KCpser*PSA_kcatC*cell*serC/(PSA_KCphp*PSA_KCpser + PSA_KCphp*pser + PSA_KCpser*php)) pser
PSA_rev: pser ->(k_PSA_rev) php
PSP: pser ->(PSP_KBser*PSP_kcatB*cell*serB/(PSP_KBpser*PSP_KBser + PSP_KBpser*ser + PSP_KBser*pser)) ser
PSP_rev: ser ->(k_PSP_rev) pser
