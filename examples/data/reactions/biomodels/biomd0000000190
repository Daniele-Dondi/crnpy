Ornithine_decarboxylase: L_Ornithine ->(Vmaxodc*cytosol/(L_Ornithine + ODC_Kmodc*(1 + Putrescine/ODC_Kipodc))) Putrescine
S_adenosylmethionine_decarboxylase: S_adenosyl_L_methionine ->(Vmaxsamdc*cytosol/((1 + Spermine/SAMdc_Kissamdc)*(SAMdc_Kmsamdc*(1 + S_adenosylmethioninamine/SAMdc_Kiasamdc + SAMdc_Kapsamdc/Putrescine) + S_adenosyl_L_methionine))) S_adenosylmethioninamine
Spermidine_Spermine_N1_acetyltransferase_for_S: Acetyl_CoA + Spermine ->(Vmaxssat*cytosol/(C*(Acetyl_CoA*SSAT_for_S_Kmsssat*(1 + Spermidine/SSAT_for_S_Kmdssat) + Acetyl_CoA*Spermine + SSAT_for_S_Kmaccoassat*SSAT_for_S_Kmsssat*(1 + Spermidine/SSAT_for_S_Kmdssat)*(CoA/SSAT_for_S_Kmcoassat + 1) + SSAT_for_S_Kmaccoassat*Spermine*(CoA/SSAT_for_S_Kmcoassat + 1)))) CoA + N1_Acetylspermine
Spermidine_spermine_N1_acetyltransferase_for_D: Acetyl_CoA + Spermidine ->(Vmaxssat*cytosol/(Acetyl_CoA*SSAT_for_D_Kmdssat*(1 + Spermine/SSAT_for_D_Kmsssat) + Acetyl_CoA*Spermidine + SSAT_for_D_Kmaccoassat*SSAT_for_D_Kmdssat*(1 + Spermine/SSAT_for_D_Kmsssat)*(CoA/SSAT_for_D_Kmcoassat + 1) + SSAT_for_D_Kmaccoassat*Spermidine*(CoA/SSAT_for_D_Kmcoassat + 1))) CoA + N1_Acetylspermidine
Polyamine_oxidase_for_aD: N1_Acetylspermidine ->(PAO_for_aD_Vmpao*cytosol/(PAO_for_aD_Kmadpao*(N1_Acetylspermidine/PAO_for_aD_Kmadpao + N1_Acetylspermine/PAO_for_aD_Kmaspao + 1 + Spermine/PAO_for_aD_Kmspao + Spermidine/PAO_for_aD_Kmdpao))) Putrescine
Polyamine_oxidase_for_aS: N1_Acetylspermine ->(PAO_for_aS_Vmpao*cytosol/(PAO_for_aS_Kmaspao*(N1_Acetylspermidine/PAO_for_aS_Kmadpao + N1_Acetylspermine/PAO_for_aS_Kmaspao + 1 + Spermine/PAO_for_aS_Kmspao + Spermidine/PAO_for_aS_Kmdpao))) Spermidine
Spermidine_synthase: Putrescine + S_adenosylmethioninamine ->(SpdS_Vmspds*cytosol/(Putrescine*S_adenosylmethioninamine + Putrescine*SpdS_KaSpds*(1 + Spermidine/SpdS_Kidspds) + S_adenosylmethioninamine*SpdS_Kpspds + SpdS_Kiaspds*SpdS_Kpspds*(1 + Spermidine/SpdS_Kidspds))) Spermidine
Spermine_synthase: S_adenosylmethioninamine + Spermidine ->(SpmS_Vmspms*cytosol/(S_adenosylmethioninamine*Spermidine + S_adenosylmethioninamine*SpmS_Kdspms + Spermidine*SpmS_Kaspms*(Spermine/SpmS_Kisspms + 1) + SpmS_Kdspms*SpmS_Kiaspms*(Spermine/SpmS_Kisspms + 1))) Spermine
Methionine_adenosyltransferase: Methionine ->(MAT_Vmmat*cytosol/(Methionine*(MAT_Kmmat*(1 + S_adenosyl_L_methionine/MAT_Kimetmat)/Methionine + 1))) S_adenosyl_L_methionine
VCOA: Acetyl_CoA ->(Kcoa*cytosol) CoA
VACCOA: CoA ->(Kaccoa*cytosol) Acetyl_CoA
Putrescine_efflux: Putrescine ->(P_efflux_Kpefflux*cytosol) 
aD_efflux: N1_Acetylspermidine ->(aD_efflux_Kadefflux*cytosol) 
